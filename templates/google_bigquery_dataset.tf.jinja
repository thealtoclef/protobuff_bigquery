resource "google_bigquery_dataset" "{{ dataset_id }}" {
  delete_contents_on_destroy = true
  project                    = local.project_id
  location                   = local.region
  dataset_id                 = "{{ dataset_id }}"

  labels = {
    "managed-by" = "terraform"
    "corpgroup"  = "cake-data",
    "data_zone"  = "raw_zone"
  }
}
